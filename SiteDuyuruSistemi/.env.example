# ================================
# MySQL Database
# ================================
MYSQL_USERNAME=siteduyuru_user
MYSQL_PASSWORD=your_secure_password_here

# ================================
# JWT Secret (minimum 64 karakter)
# ================================
JWT_SECRET=your_super_secret_jwt_key_minimum_64_characters_long_for_security

# ================================
# Gemini AI API Key
# ================================
GEMINI_API_KEY=your_gemini_api_key_here

# ================================
# Stripe API Key  
# ================================
STRIPE_API_KEY=your_stripe_api_key_here
